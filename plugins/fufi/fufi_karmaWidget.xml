<!--start_header-->
<?xml version="1.0" encoding="UTF-8"?>
 <manialinks id='%manialinksid%'>
   <manialink id='%widgetid%'>
   <frame posn="%widgetpos%">
   <!--<frame posn="49.5 15 0">-->

<!--end_header-->

<!--start_window-->
     <quad sizen="12 5.8" posn="0 0 0.001" style="%style%" substyle="%substyle%"/>
     <format textsize="1"/>
<!--end_window--> 

<!--start_title-->
     <quad sizen="12 2" halign="center" posn="%x% 1.7 0.001" style="%style%" substyle="%substyle%"/>
     <label sizen="30 2" halign="center" posn="%x% 1.5 0.1" text="$s%widgettitle%"/>
<!--end_title-->

<!--start_minus-->
     <quad sizen="2.8 2.8" posn="3 -2.5 0.1" %action% style="Icons64x64_1" substyle="SliderCursor"/>
     <format textsize="2"/>
     <label sizen="10 2" halign="center" posn="4.4 -2.6 0.1" text="$s$w$f00-"/>
<!--end_minus-->

<!--start_neglabel-->
     <label sizen="2.2 2" halign="right" posn="3.05 -3.1 0.1" text="$f00%text%"/>
<!--end_neglabel-->

<!--start_poslabel-->
	 <label sizen="2.2 2" halign="left" posn="8.1 -3.1 0.1" text="$0f0%text%"/>
<!--end_poslabel-->

<!--start_plus-->
     <quad sizen="2.8 2.8" posn="5.4 -2.5 0.1" %action% style="Icons64x64_1" substyle="SliderCursor"/>
     <format textsize="2"/>
     <label sizen="10 2" halign="center" posn="6.8 -2.9 0.1" text="$s$0f0+"/>
<!--end_plus-->

<!--start_highlight_minus-->
     <quad sizen="2.8 2.8" posn="3.01 -2.5 0.09" style="Icons64x64_1" substyle="LvlGreen"/>
<!--end_highlight_minus-->

<!--start_highlight_plus-->
     <quad sizen="2.8 2.8" posn="5.39 -2.5 0.09" style="Icons64x64_1" substyle="LvlGreen"/>
<!--end_highlight_plus-->

<!--start_award-->
      <quad sizen="%width% %width%" valign="bottom" halign="center" posn="%x% %y% 0.2" style="Icons64x64_1" substyle="OfficialRace"/>
<!--end_award--> 

<!--start_glow_award-->
      <quad sizen="%width% %width%" valign="bottom" halign="center" posn="%x% %y% 0.2" style="Icons128x32_1" substyle="RT_Cup"/>
<!--end_glow_award--> 



<!--start_highlight_entry-->
     <quad sizen="20 2.2" posn="0.1 %y% 0.002" style="BgsPlayerCard" substyle="BgCardSystem"/>
     <frame posn="0 %y% 0">
      <quad sizen="2.2 2.7" posn="-1.3 0.25 0.2" style="Icons64x64_1" substyle="ArrowGreen"/>
     </frame>
<!--end_highlight_entry--> 
	 
<!--start_footer-->
    </frame>
 </manialink>
 </manialinks>
<!--end_footer-->